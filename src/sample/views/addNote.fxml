<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.views.AddNoteController">
   <children>
      <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="200.0" prefWidth="400.0">
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="textArea" layoutX="6.0" layoutY="10.0" prefHeight="127.0" prefWidth="385.0" promptText="Wprowadź notatkę tutaj..." />
               </children>
            </Pane>
 
            <HBox alignment="TOP_CENTER" prefHeight="74.0" prefWidth="400.0" spacing="150.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#cancel" onKeyReleased="#cancel" stylesheets="@styles.css" text="Anuluj">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <styleClass>
                        <String fx:value="button" />
                        <String fx:value="choice_button" />
                     </styleClass></Button>
                  <Button mnemonicParsing="false" onAction="#accept" stylesheets="@styles.css" text="OK">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                     <styleClass>
                        <String fx:value="button" />
                        <String fx:value="choice_button" />
                     </styleClass>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
               </VBox.margin></HBox>
         </children></VBox>
   </children>
</AnchorPane>
